<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#ffffdf5b" />
            <stroke
                android:width="1dp"
                android:color="#ffffa711" />
            <corners
                android:radius="8dp" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#ffffdf5b"
                android:endColor="#ffffb227"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#fff4a146" />
            <corners
                android:radius="8dp" />

        </shape>
    </item>
</selector>